<?xml version="1.1" encoding="UTF-8"?>

<rosetta>
    <namespace name="paletter">
        <var name="color_types" type="Map" access="public">
            <generic>String</generic>
            <generic>ColorBase</generic>
        </var>
        <function access="public" name="is_valid_color" ret-type="Bool">
            <var name="color_code" />
            <var name="max_range" />
            <var name="min_range">0</var>
            <var type="Number" />
            <body>
                <ret>
                    <operator name="&lt;">
                        <read-var name="color_code" />
                        <read-var name="max_range" />
                        <read-var name="min_range" />
                    </operator>
                </ret>
            </body>
        </function>
        <class access="public" name="ColorBase" meta="">
            <var name="color_code" />
            <var name="String" />
            <child-declaration></child-declaration>
        </class>
    </namespace>
</rosetta>
